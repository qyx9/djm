
.topNav_content{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
	width: 100%;
}
.topNav_type{
	width: 25%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	float: left;
	text-align: center;
	font-size: 24rpx;
	border-right: 4rpx solid #F0F0F0;
	border-top: 2rpx solid #F0F0F0;
	padding: 10rpx 24rpx;
}
.topNav_type.last{
	border-right: 0px solid;
}
.topNav_type text {
	padding-top: 10rpx;
	padding-bottom: 10rpx;
	display: inline-block;
}
.topNav_type image{
	margin: 0 auto;
	width: 100rpx;
	height: 100rpx;
	display: block;
}
.hot_issuesTle{
	background: #F0F0F0;
	border-bottom: 4rpx solid #D0D0D0;
	width: 100%;
	height: 110rpx;
	line-height: 140rpx;
	font-size: 30rpx;
	font-weight: bold;
}
.hot_issuesTle text {
	padding-left: 26rpx;
}
.hotIssues_href{
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	font-size: 28rpx;
	font-weight: bold;
	border-bottom: 2rpx solid #D0D0D0;
}
.hotIssues_href.last{
	border-bottom: 4rpx solid #D0D0D0;
}
.hotIssues_href image{
	width: 62rpx;
	height: 62rpx;
	vertical-align: middle;
}
.hotIssues_href navigator{
	text-indent: 16px;
	display: inline-block;
	width: 100%;
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-position: center right;
}
.placeholder_bg{
	width: 100%;
	height: 20rpx;
	background: #EEEEEE;
}
.workingDay{
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	border-bottom: 2rpx solid #D0D0D0;
	font-size: 30rpx;
}
.workingDay_lt{
	color: #cc3065;
	display: inline-block;
	width: 50%;
	float: left;
	padding-left: 26rpx;
}
.workingDay_rt{
	color: #969696;
	display: inline-block;
	width: 42%;
	text-align: right;
}
.service_href{
	width: 100%;
	height: 90rpx;
	line-height: 90rpx;
	font-size: 30rpx;
}
.service_lt{
	display: inline-block;
	width: 50%;
	float: left;
	padding-left: 26rpx;
}
.service_rt{
	color: #969696;
	display: inline-block;
	width: 42%;
	text-align: right;
}

